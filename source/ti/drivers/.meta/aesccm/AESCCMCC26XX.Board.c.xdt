/*
 *  ======== AESCCMCC26XX.Board.c.xdt ========
 */

    let AESCCM = args[0];
    let Common = system.getScript("/ti/drivers/Common.js");

    let instances = AESCCM.$instances;

    if (instances.length == 0) return;

    /* Board_XXXCOUNT string, derived from defaultInstanceName */
    let countDef = AESCCM.defaultInstanceName + "COUNT";

%%}
/*
 *  =============================== AESCCM ===============================
 */

#include <ti/drivers/AESCCM.h>
#include <ti/drivers/aesccm/AESCCMCC26XX.h>

#define `countDef` `instances.length`

AESCCMCC26XX_Object aesccmCC26XXObjects[`countDef`];

/*
 *  ======== aesccmCC26XXHWAttrs ========
 */
const AESCCMCC26XX_HWAttrs aesccmCC26XXHWAttrs[`countDef`] = {
% for (let i = 0; i < instances.length; i++) {
%     let inst        = instances[i];
    {
        .intPriority = `Common.intPriority2Hex(inst.interruptPriority)`,
    },
% }
};

const AESCCM_Config AESCCM_config[`countDef`] = {
% for (let i = 0; i < instances.length; i++) {
%     let inst    = instances[i];
    {   /* `inst.$name` */
        .object  = &aesccmCC26XXObjects[`inst.$name`],
        .hwAttrs = &aesccmCC26XXHWAttrs[`inst.$name`]
    },
% }
};

const uint_least8_t AESCCM_count = `countDef`;
