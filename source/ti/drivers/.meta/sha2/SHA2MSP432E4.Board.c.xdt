/*
 *  ======== SHA2MSP432E4.Board.c.xdt ========
 */

    let SHA2    = args[0];
    let Common    = system.getScript("/ti/drivers/Common.js");

    let instances = SHA2.$instances;

    if (SHA2.$instances.length == 0) return;

    /* Board_XXXCOUNT string, derived from defaultInstanceName */
    let countDef = SHA2.defaultInstanceName + "COUNT";

%%}
/*
 *  =============================== SHA2 ===============================
 */

#include <ti/drivers/SHA2.h>
#include <ti/drivers/sha2/SHA2MSP432E4.h>

#define `countDef` `instances.length`

SHA2MSP432E4_Object sha2MSP432E4Objects[`countDef`];

/*
 *  ======== sha2MSP432E4HWAttrs ========
 */
const SHA2MSP432E4_HWAttrs sha2MSP432E4HWAttrs[`countDef`] = {
% for (let i = 0; i < instances.length; i++) {
%     let inst        = instances[i];
    {
        .intPriority = `Common.intPriority2Hex(inst.interruptPriority)`,
    },
% }
};

const SHA2_Config SHA2_config[`countDef`] = {
% for (let i = 0; i < instances.length; i++) {
%     let inst    = instances[i];
    {   /* `inst.$name` */
        .object         = &sha2MSP432E4Objects[`inst.$name`],
        .hwAttrs        = &sha2MSP432E4HWAttrs[`inst.$name`]
    },
% }
};

const uint_least8_t SHA2_count = `countDef`;
