<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<gep name="ClockFreqs" title="ClockFreqs Module" version="1">

<page name="Module" title="Module Settings">
<html><body><table>
  <tr>
      <td><form>
        <p>The ClockFreqs module is used to define the ACLK, SMCLK, and HSMCLK frequencies in effect when BIOS_start() is called at the end of main().</p>
        <p> </p>
      </form></td>
  </tr>
  <tr>
    <td><control type="checkbox" label="Add the ClockFreqs module to my configuration" value="value:this.$used" bold="true"/></td>
  </tr>
  <tr></tr>
  <tr><td>
    <h1>Clock Frequencies</h1>
    <table>
      <tr>
        <td><control type="label" label="ACLK frequency (Hz)" align="left"/></td>
        <td><control type="editbox" 
             value="value:this.ACLK" 
             tooltip="value:this.ACLK.$summary"/></td>
      </tr>
      <tr>
        <td><control type="label" label="SMCLK frequency (Hz)" align="left"/></td>
        <td><control type="editbox" 
             value="value:this.SMCLK" 
             tooltip="value:this.SMCLK.$summary"/></td>
      </tr>
      <tr>
        <td><control type="label" label="HSMCLK frequency (Hz)" align="left"/></td>
        <td><control type="editbox" 
             value="value:this.HSMCLK" 
             tooltip="value:this.HSMCLK.$summary"/></td>
      </tr>
    </table>
  </td></tr>
</table></body></html>
</page>
<properties name="Advanced" title="All Options" enabled="true" visible="true" link="this"/>
</gep>

