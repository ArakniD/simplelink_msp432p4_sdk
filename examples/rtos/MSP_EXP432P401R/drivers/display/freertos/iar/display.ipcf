<?xml version="1.0" encoding="UTF-8"?>
<!-- IAR Project Connection File -->

<iarProjectConnection version="1.2" name="Display" oneShot="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="IarProjectConnection_1.2.xsd">

  <device>
    <name>MSP432P401R</name>
  </device>

  <extraOptions>
    <args>
      <arg>-I$PROJ_DIR$</arg>
      <arg>-DBOARD_DISPLAY_USE_UART_ANSI=1</arg>
      <arg>-DBOARD_DISPLAY_USE_LCD=1</arg>
      <arg>-I$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source</arg>
      <arg>-I$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/third_party/CMSIS/Include</arg>
      <arg>-I$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/posix/iar</arg>
      <arg>-I$FREERTOS_INSTALL_DIR$/FreeRTOS/Source/include</arg>
      <arg>-I$FREERTOS_INSTALL_DIR$/FreeRTOS/Source/portable/IAR/ARM_CM4F</arg>
      <arg>-I$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/kernel/freertos/builds/MSP_EXP432P401R/release</arg>
      <arg>--debug</arg>
      <arg>--silent</arg>
      <arg>-e</arg>
      <arg>--aeabi</arg>
      <arg>--thumb</arg>
      <arg>--diag_suppress=Pa050</arg>
    </args>
  </extraOptions>

  <asmIncludePath>
    <path>$PROJ_DIR$</path>
    <path>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/kernel/freertos/builds/MSP_EXP432P401R/release</path>
  </asmIncludePath>

  <asmDefines>
  </asmDefines>

  <linkerFile>
    <override>true</override>
    <path>$PROJ_DIR$/$PROJ_FNAME$_MSP_EXP432P401R_FREERTOS.icf</path>
  </linkerFile>

  <linkerExtraOptions>
    <args>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/display/lib/display.arm4f</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/grlib/lib/iar/m4f/grlib.a</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/third_party/spiffs/lib/iar/m4f/spiffs.a</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/drivers/lib/drivers_msp432p401x.arm4f</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/third_party/fatfs/lib/iar/m4f/fatfs.a</arg>
      <arg>--threaded_lib</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/kernel/freertos/builds/MSP_EXP432P401R/release/iar/freertos.lib</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/devices/msp432p4xx/driverlib/iar/msp432p4xx_driverlib.a</arg>
      <arg>--silent</arg>
      <arg>--cpu=Cortex-M4F</arg>
    </args>
  </linkerExtraOptions>

    <customBuildTools>
    </customBuildTools>

  <heaps>
    <heap> <!-- If omitted, id="0" -->
      <size>0</size>
    </heap>
    <heap id="1">
      <size>0</size>
    </heap>
  </heaps>
  <stacks>
    <stack> <!-- If omitted, id="0" -->
      <size>512</size>
    </stack>
  </stacks>

  <preBuild>
    <command>iarbuild.exe $SIMPLELINK_MSP432_SDK_INSTALL_DIR$/kernel/freertos/builds/MSP_EXP432P401R/release/iar/freertos_builds_MSP_EXP432P401R_release.ewp Debug</command>
  </preBuild>

  <files>
    <group name="Source files">
      <path copyTo="$PROJ_DIR$/main_freertos.c">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/freertos/main_freertos.c</path>
      <path copyTo="$PROJ_DIR$/display.c">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/display.c</path>
      <path copyTo="$PROJ_DIR$/splash_image.c">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/splash_image.c</path>
      <path copyTo="$PROJ_DIR$/MSP_EXP432P401R.c">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/MSP_EXP432P401R.c</path>
      <path copyTo="$PROJ_DIR$/$PROJ_FNAME$_MSP_EXP432P401R_FREERTOS.icf">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/freertos/iar/MSP_EXP432P401R_FREERTOS.icf</path>
    </group>

    <group name="Header files">
      <path copyTo="$PROJ_DIR$/splash_image.h">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/splash_image.h</path>
      <path copyTo="$PROJ_DIR$/Board.h">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/Board.h</path>
      <path copyTo="$PROJ_DIR$/MSP_EXP432P401R.h">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/MSP_EXP432P401R.h</path>
    </group>

    <group name="Documentation" build="false">
      <path copyTo="$PROJ_DIR$/README.md">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/README.md</path>
      <path copyTo="$PROJ_DIR$/README.html">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/rtos/MSP_EXP432P401R/drivers/display/README.html</path>
      <path reference="$PROJ_DIR$/Board.html">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/boards/MSP_EXP432P401R/Board.html</path>
    </group>
  </files>
</iarProjectConnection>
