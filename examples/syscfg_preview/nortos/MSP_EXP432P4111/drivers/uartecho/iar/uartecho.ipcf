<?xml version="1.0" encoding="UTF-8"?>
<!-- IAR Project Connection File -->

<iarProjectConnection version="1.2" name="UART Echo" oneShot="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="IarProjectConnection_1.2.xsd">

  <device>
    <name>MSP432P4111</name>
  </device>

  <extraOptions>
    <args>
      <arg>-I$PROJ_DIR$</arg>
      <arg>-I$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source</arg>
      <arg>-I$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/third_party/CMSIS/Include</arg>
      <arg>-I$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/kernel/nortos</arg>
      <arg>-I$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/kernel/nortos/posix</arg>
      <arg>--debug</arg>
      <arg>--silent</arg>
      <arg>-e</arg>
      <arg>--aeabi</arg>
      <arg>--thumb</arg>
      <arg>--diag_suppress=Pa050</arg>
    </args>
  </extraOptions>

  <asmIncludePath>
    <path>$PROJ_DIR$</path>
  </asmIncludePath>

  <asmDefines>
  </asmDefines>

  <linkerFile>
    <override>true</override>
    <path>$PROJ_DIR$/$PROJ_FNAME$_MSP_EXP432P4111_NoRTOS.icf</path>
  </linkerFile>

  <linkerExtraOptions>
    <args>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/display/lib/display.arm4f</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/grlib/lib/iar/m4f/grlib.a</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/third_party/spiffs/lib/iar/m4f/spiffs.a</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/drivers/lib/drivers_msp432p4x1xi.arm4f</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/third_party/fatfs/lib/iar/m4f/fatfs.a</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/kernel/nortos/lib/nortos_msp432p4x1xi.arm4f</arg>
      <arg>$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/devices/msp432p4xx/driverlib/iar/msp432p4xx_driverlib.a</arg>
      <arg>--silent</arg>
      <arg>--cpu=Cortex-M4F</arg>
    </args>
  </linkerExtraOptions>

    <customBuildTools>
    <customBuildTool atRoot="true" name="SysConfig"> <!-- Custom build tool for entire project-->
        <fileExtensions>.syscfg</fileExtensions>
        <command>$SYSCONFIG_ROOT$/nw/nw.exe $SYSCONFIG_ROOT$/dist/cli.js -o $PROJ_DIR$ -s "$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/.metadata/product.json" --compiler iar $FILE_PATH$
        </command>
        <output>
            <path>SysConfig_Generated_Files.ipcf</path>
        </output>
        <hasPrio>true</hasPrio> <!-- true: run this tool before all other tools-->
    </customBuildTool>
    </customBuildTools>

  <heaps>
    <heap> <!-- If omitted, id="0" -->
      <size>0</size>
    </heap>
    <heap id="1">
      <size>0</size>
    </heap>
  </heaps>
  <stacks>
    <stack> <!-- If omitted, id="0" -->
      <size>512</size>
    </stack>
  </stacks>


  <files>
    <group name="Source files">
      <path copyTo="$PROJ_DIR$/uartecho.syscfg">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/syscfg_preview/nortos/MSP_EXP432P4111/drivers/uartecho/uartecho.syscfg</path>
      <path copyTo="$PROJ_DIR$/main_nortos.c">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/syscfg_preview/nortos/MSP_EXP432P4111/drivers/uartecho/main_nortos.c</path>
      <path copyTo="$PROJ_DIR$/uartecho.c">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/syscfg_preview/nortos/MSP_EXP432P4111/drivers/uartecho/uartecho.c</path>
      <path copyTo="$PROJ_DIR$/$PROJ_FNAME$_MSP_EXP432P4111_NoRTOS.icf">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/syscfg_preview/nortos/MSP_EXP432P4111/drivers/uartecho/iar/MSP_EXP432P4111_NoRTOS.icf</path>
    </group>

    <group name="Header files">
    </group>

    <group name="Documentation" build="false">
      <path copyTo="$PROJ_DIR$/README.md">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/syscfg_preview/nortos/MSP_EXP432P4111/drivers/uartecho/README.md</path>
      <path copyTo="$PROJ_DIR$/README.html">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/examples/syscfg_preview/nortos/MSP_EXP432P4111/drivers/uartecho/README.html</path>
      <path reference="$PROJ_DIR$/Board.html">$SIMPLELINK_MSP432_SDK_INSTALL_DIR$/source/ti/boards/MSP_EXP432P4111/Board.html</path>
    </group>
  </files>
</iarProjectConnection>
